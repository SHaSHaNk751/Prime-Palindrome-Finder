a=int(input("Enter your number"))
b=0
for i in range(2,a):
    if a%i==0:
        b=1
        break
if b==1:
    print(a,"is not a Prime")
else:
    x=a
    sum=0
    while x>0:
        r=x%10
        sum=sum*10+r
        x=x//10
    if sum==a:
        print(a,"is a Prime Palindrome")
    else:
        print(a,"is a Prime number but not prime Palindrome")
